<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.budowa.flow.users.AddUserScene">
   <children>
      <VBox id="vBoxContainer" layoutX="275.0" prefHeight="500.0" prefWidth="250.0" spacing="4.0" AnchorPane.bottomAnchor="16.0" AnchorPane.leftAnchor="275.0" AnchorPane.rightAnchor="275.0" AnchorPane.topAnchor="16.0">
         <children>
            <Label prefHeight="17.0" prefWidth="253.0" text="Pełna nazwa" />
            <TextField fx:id="textFieldName" onKeyPressed="#controlAction">
               <VBox.margin>
                  <Insets bottom="4.0" />
               </VBox.margin>
            </TextField>
            <Label prefHeight="16.0" prefWidth="259.0" text="Login" />
            <TextField fx:id="textFieldLogin" onKeyPressed="#controlAction">
               <VBox.margin>
                  <Insets bottom="4.0" />
               </VBox.margin></TextField>
            <Label prefHeight="16.0" prefWidth="299.0" text="Hasło" />
            <PasswordField fx:id="textFieldPassword" onKeyPressed="#controlAction">
               <VBox.margin>
                  <Insets bottom="4.0" />
               </VBox.margin></PasswordField>
            <Label prefHeight="16.0" prefWidth="251.0" text="Rola" />
            <ChoiceBox fx:id="choiceBoxRole" onInputMethodTextChanged="#controlAction" onKeyPressed="#controlAction" onMouseClicked="#controlAction" prefHeight="25.0" prefWidth="262.0">
               <VBox.margin>
                  <Insets bottom="4.0" />
               </VBox.margin>
            </ChoiceBox>
            <Button fx:id="buttonAdd" mnemonicParsing="false" onAction="#addUserButtonAction" prefHeight="25.0" prefWidth="253.0" text="Dodaj">
               <VBox.margin>
                  <Insets top="16.0" />
               </VBox.margin></Button>
         </children>
         <padding>
            <Insets bottom="8.0" top="8.0" />
         </padding></VBox>
      <Button fx:id="backButton" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#backButtonAction" text="Powrót" />
   </children>
</AnchorPane>
